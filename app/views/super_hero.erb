<h1>Create a Team and Heroes!</h1>

  <form action="/teams" methos="post">
    Team Name: <input id="team_name" type="text" name="team[name]">
    Team Motto: <input id="team_motto" type="text" name="team[motto]">
    <br>
    <h2> Hero 1</h2>
    Hero's Name: <input id="hero_name" type="text" name="team[heroes][][name]">
    Hero's Power: <input id="hero_power" type="text" name="team[heroes][][power]">
    Hero's Biography: <input id="hero_biography" type="text" name="team[heroes][][biography]">
    <h2> Hero 2</h2>
      Hero's Name: <input id="hero_name_2" type="text" name="team[heroes][][name]">
      Hero's Power: <input id="hero_power_2" type="text" name="team[heroes][][power]">
      Hero's Biography: <input id="hero_biography_2" type="text" name="team[heroes][][biography]">
    <h2>Hero 3 </h2>  
    Hero's Name: <input id="hero_name_3" type="text" name="team[heroes][][name]">
    Hero's Power: <input id="hero_power_3" type="text" name="team[heroes][][power]">
    Hero's Biography: <input id="hero_biography_3" type="text" name="team[heroes][][biography]">
    <input type="submit" id="submit">
  </form>